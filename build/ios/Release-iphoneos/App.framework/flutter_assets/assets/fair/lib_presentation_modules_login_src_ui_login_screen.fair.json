{
  "className": "CustomScaffold",
  "na": {
    "body": "%(_buildBody)"
  },
  "methodMap": {
    "inputForm": {
      "className": "Column",
      "na": {
        "mainAxisAlignment": "#(MainAxisAlignment.start)",
        "crossAxisAlignment": "#(CrossAxisAlignment.center)",
        "children": [
          {
            "className": "Container",
            "na": {
              "margin": {
                "className": "EdgeInsets.only",
                "na": {
                  "bottom": 15.0
                }
              },
              "child": {
                "className": "Text",
                "pa": [
                  "Đăng nhập"
                ],
                "na": {
                  "style": {
                    "className": "TextStyle",
                    "na": {
                      "fontSize": 22.0,
                      "color": {
                        "className": "Color",
                        "pa": [
                          "0xFF000000"
                        ]
                      },
                      "fontWeight": "#(FontWeight.bold)"
                    }
                  }
                }
              }
            }
          },
          {
            "className": "Padding",
            "na": {
              "padding": {
                "className": "EdgeInsets.symmetric"
              },
              "child": {
                "className": "CustomTextField",
                "na": {
                  "hintText": "Tài khoản SaleClub",
                  "focusNode": "#(_userNameNode)",
                  "textInputAction": "#(_textNextInputAction)",
                  "radius": 4.0
                }
              }
            }
          },
          {
            "className": "SizedBox",
            "na": {
              "height": 10.0
            }
          },
          {
            "className": "Padding",
            "na": {
              "padding": {
                "className": "EdgeInsets.symmetric"
              },
              "child": {
                "className": "CustomTextField",
                "na": {
                  "hintText": "Mật khẩu",
                  "focusNode": "#(_passwordNode)",
                  "radius": 4.0,
                  "obscureText": false
                }
              }
            }
          },
          "%(rememberAndForgetPassword)",
          {
            "className": "CustomButton",
            "na": {
              "radius": 4.0,
              "text": "Đăng nhập"
            }
          },
          {
            "className": "Center",
            "na": {
              "child": {
                "className": "Container",
                "na": {
                  "margin": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "top": 20.0
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "Bạn chưa có tài khoản"
                    ],
                    "na": {
                      "style": {
                        "className": "TextStyle",
                        "na": {
                          "fontSize": 14.0,
                          "color": {
                            "className": "Color",
                            "pa": [
                              "0xFF000000"
                            ]
                          },
                          "fontWeight": "#(FontWeight.w400)"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "className": "Container",
            "na": {
              "height": 40.0,
              "alignment": "#(Alignment.center)",
              "child": {
                "className": "Center",
                "na": {
                  "child": {
                    "className": "Text",
                    "pa": [
                      "Đăng ký đối tác kinh doanh"
                    ],
                    "na": {
                      "style": {
                        "className": "TextStyle",
                        "na": {
                          "fontSize": 14.0,
                          "color": {
                            "className": "Color",
                            "pa": [
                              "0xFF2F80ED"
                            ]
                          },
                          "decoration": "#(TextDecoration.underline)",
                          "fontWeight": "#(FontWeight.w400)"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "className": "SizedBox",
            "na": {
              "height": 10
            }
          }
        ]
      }
    },
    "rememberAndForgetPassword": {
      "className": "Container",
      "na": {
        "margin": {
          "className": "EdgeInsets.only",
          "na": {
            "top": 10.0,
            "bottom": 10.0,
            "left": 10.0
          }
        },
        "child": {
          "className": "Row",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.start)",
            "crossAxisAlignment": "#(CrossAxisAlignment.center)",
            "children": [
              {
                "className": "Flexible",
                "na": {
                  "flex": 2,
                  "child": {
                    "className": "Row",
                    "na": {
                      "children": [
                        {
                          "className": "CustomCheckbox",
                          "na": {
                            "value": false,
                            "onChanged": "#(_onChangedValue)"
                          }
                        },
                        {
                          "className": "Expanded",
                          "na": {
                            "child": {
                              "className": "Container",
                              "na": {
                                "margin": {
                                  "className": "EdgeInsets.only",
                                  "na": {
                                    "left": 5.0
                                  }
                                },
                                "child": {
                                  "className": "Text",
                                  "pa": [
                                    "Nhớ mật khẩu"
                                  ]
                                }
                              }
                            }
                          }
                        }
                      ]
                    }
                  }
                }
              },
              {
                "className": "Flexible",
                "na": {
                  "flex": 1,
                  "child": {
                    "className": "Container"
                  }
                }
              },
              {
                "className": "Flexible",
                "na": {
                  "flex": 2,
                  "child": {
                    "className": "Container",
                    "na": {
                      "height": 40.0,
                      "alignment": "#(Alignment.center)",
                      "child": {
                        "className": "Center",
                        "na": {
                          "child": {
                            "className": "Text",
                            "pa": [
                              "Quên mật khẩu"
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              }
            ]
          }
        }
      }
    },
    "_buildBody": {
      "className": "SingleChildScrollView",
      "na": {
        "controller": "#(_controller)",
        "child": {
          "className": "Column",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.start)",
            "crossAxisAlignment": "#(CrossAxisAlignment.start)",
            "children": [
              {
                "className": "Center",
                "na": {
                  "child": {
                    "className": "Align",
                    "na": {
                      "alignment": "#(Alignment.center)",
                      "child": {
                        "className": "Container",
                        "na": {
                          "margin": {
                            "className": "EdgeInsets.only"
                          },
                          "child": {
                            "className": "Image.asset",
                            "pa": [
                              "assets/images/img_logo_saleclub.png"
                            ],
                            "na": {
                              "fit": "#(BoxFit.fitWidth)"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              },
              "%(inputForm)",
              {
                "className": "Center",
                "na": {
                  "child": {
                    "className": "Container",
                    "na": {
                      "margin": {
                        "className": "EdgeInsets.only",
                        "na": {
                          "top": 10.0
                        }
                      },
                      "height": 25.0,
                      "alignment": "#(Alignment.center)",
                      "child": {
                        "className": "Image.asset",
                        "pa": [
                          "assets/icons/ic_fpt.png"
                        ]
                      }
                    }
                  }
                }
              },
              {
                "className": "Center",
                "na": {
                  "child": {
                    "className": "Container",
                    "na": {
                      "margin": {
                        "className": "EdgeInsets.only",
                        "na": {
                          "top": 20.0,
                          "bottom": 20.0
                        }
                      },
                      "child": {
                        "className": "Text",
                        "pa": [
                          "Version"
                        ]
                      }
                    }
                  }
                }
              }
            ]
          }
        }
      }
    }
  }
}